#dashboard-push-overlay {
    .yui3-widget-bd {
        .tabs {
            position:absolute;
            top:1em;
            right:-80px;
            width:80px;
            .tab {
                background:rgb(220,220,220);
                -webkit-border-top-right-radius:5px;
                -webkit-border-bottom-right-radius:5px;
                opacity:.7;
                width:80%;
                cursor:pointer;
                color:#666;
                -webkit-transition-property:width;
                -webkit-transition-duration:0.1s;
                &.active {
                    -webkit-box-shadow:6px 0px 0px #435d6f;
                    opacity:1;
                    width:100%;
                    cursor:default;
                    z-index:999;
                    color:#333;
                    .inner {
                        text-shadow:1px 1px 1px #fff;
                        font-weight:bold;
                    }
                }
                &:not(.active):hover {
                    width:90%;
                }
                .inner {
                    text-align:right;
                    padding:6px 3px;
                }
            }
        }
        table {
            width:100%;
            max-height:65%;
            thead {
                tr {
                    th {
                        min-width:5em;
                        font-weight:normal;
                        text-shadow:-1px -1px 0 rgba(255, 255, 255, 0.6);
                        padding-bottom:4px;
                    }
                }
            }
            tbody {
                border:1px solid #8d8d8d;
                border-bottom-color:#a6a2a2;
                border-right-color:#a6a2a2;
                background:#fff;
                tr {
                    color:#333;
                    -webkit-transition-property:background-color;
                    -webkit-transition-duration:0.3s;
                    &:hover, &.selected {
                        background:#ecf1f4;
                        color:#116EB3;
                    }
                    &.selected {
                        font-weight:bold;
                    }
                    td {
                        padding:2px;
                        text-align:center;
                        cursor:default;
                        /*
                        &:first-child {
                            text-align:left;
                        }
                        */
                        &[colspan] {
                            text-align:center;
                            font-weight:bold;
                            padding:1em 0;
                            &:hover {
                                background:#fff;
                                color:#333;
                            }
                        }
                    }
                }
            }
        }
        .notice {
            p, .icon, .loader {
                display:inline-block;
            }
            .loader {
                width:16px;
                height:11px;
                background:url(/images/loader-1.gif) center center no-repeat;
                position:relative;
                top:2px;
                margin-left:1em;
                opacity:0;
                -webkit-transition-property:opacity;
                -webkit-transition-duration:0.3s;
            }
            .icon {
                background:url(@image_chat_dots) left center no-repeat;
                background-size:100%;
                width:16px;
                height:16px;
                margin-right:.5em;
                position:relative;
                top:3px;
            }
            p {
                color:#333;
                font-size:10px;
            }
        }
        .filters {
            margin-top:5px;
            color:#333;
            h5 {
                font-size:11px;
                display:inline-block;
                margin-right:10px;
                color:#222;
            }
            label {
                span {
                    font-size:10px;
                    display:inline-block;
                    -webkit-border-radius:5px;
                    padding:3px;
                }
                input[type=checkbox] {
                    display:none;
                    &:checked + span {
                        color:#fff;
                        background:#435d6f;
                    }
                }
            }
        }
        .separator {
            height:0;
            font-size:0;
            overflow:hidden;
            border-top:1px solid #888;
            border-bottom:1px solid #f3f3f3;
        }
        .details {
            width:100%;
            position:relative;
            .outer {
                height:100%;
                .inner {
                    min-height:2em;
                    padding:1em;
                    h4 {
                        font-size:13px;
                        margin-top:5px;
                    }
                    p {
                        color:#666;
                        text-align:justify;
                        text-overflow:ellipsis;
                        white-space:nowrap;
                        overflow:hidden;
                        &:hover {
                            white-space:normal;
                        }
                        &:first-line {
                            color:#444;
                        }
                    }
                    .files {
                        margin-top:15px;
                        ul {
                            list-style-type:none;
                            height:7em;
                            overflow:auto;
                            border:1px solid #8d8d8d;
                            border-bottom-color:#a6a2a2;
                            border-right-color:#a6a2a2;
                            background:#fff;
                            padding:5px;
                            li {
                                color:#444;
                                cursor:default;
                                -webkit-transition-property:background-color;
                                -webkit-transition-duration:0.3s;
                                &:hover {
                                    background-color:#e3e3e3;
                                }
                            }
                        }
                    }
                    .actions {
                        position:absolute;
                        top:1.3em;
                        right:0;
                        input {
                            border:0;
                            font-size:11px;
                            color:#000;
                            width:10em;
                            outline:0;
                            position:relative;
                            top:-8px;
                            margin-right:15px;
                            padding-left:2px;
                            background-color:transparent;
                            background-image:-webkit-gradient(linear, left top, right top, color-stop(0, rgba(255,255,255,0.7)), color-stop(1, rgba(255,255,255,0)));
                            -webkit-transition-property:opacity;
                            -webkit-transition-duration:0.2s;
                            opacity:0;
                            &:focus {
                                width:20em;
                            }
                        }
                        span {
                            width:24px;
                            height:24px;
                            display:inline-block;
                            background-repeat:no-repeat;
                            background-position:center center;
                            cursor:pointer;
                            -webkit-transition-property:background-size;
                            -webkit-transition-duration:0.1s;
                            &.yes {
                                background-image:url(@image_ok);
                                background-size:70%;
                                position:relative;
                                top:-2px;
                                &:hover {
                                    background-size:100%;
                                }
                            }
                            &.no {
                                background-image:url(@image_close);
                                background-size:140%;
                                &:hover {
                                    background-size:180%;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}