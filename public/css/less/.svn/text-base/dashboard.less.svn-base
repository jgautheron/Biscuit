.biscuit-dashboard {
    display:-webkit-box;
    -webkit-box-pack:center;
    -webkit-box-orient:vertical;
    -webkit-box-align:stretch;
    text-align:center;
    -webkit-box-flex:1;
    background:#114F8D url(../images/biscuit-bg.jpg) no-repeat center center;

    h1 {
        background:url(../images/logo.png) no-repeat center left;
        display:block;
        margin:0 auto;
        width:511px;
        height:241px;
        position:relative;
        top:0;
        &:after {
            content:"Dashboard";
            color:#fff;
            font-family:VegurLight;
            font-size:48px;
            padding-left:5.5em;
            line-height:7.8em;
            text-shadow:-1px -1px 5px rgba(0, 0, 0, 0.5);
        }
    }

    .logo {
        margin:-10em 0 30px 0;
    }

    .icon {
        display:block;
        position:absolute;
        width:78px;
        height:78px;
        background-repeat:no-repeat;
        cursor:pointer;
        padding-bottom:30px;
        -webkit-border-radius:5px;
        -webkit-transition-property:background-color;
        -webkit-transition-duration:0.3s;

        &:after {
            background:rgba(25, 105, 178, 0.7);
            padding:2px 14px;
            -webkit-border-radius:5px;
            font:normal 12px DroidSans;
            color:#fff;
            position:relative;
            top:86px;
            text-shadow:-1px -1px 1px rgba(0, 0, 0, 0.5);
        }
        &:hover {
            background-color:rgba(25, 105, 178, 0.5);
            &:after {
                background-color:transparent;
            }
        }
        &.drag {
            cursor:move;
        }
        &.out {
            &:before {
                content:"";
                background-image:url(../images/right.png);
                width:32px;
                height:32px;
                position:absolute;
                top:0;
                right:0;
            }
        }

        &.push {
            background-image:url(../images/cake1.png);
            bottom:20%;
            left:40%;
            &:after {
                content:"Delivery";
            }
        }

        &.editor {
            background-image:url(../images/cake2.png);
            bottom:20%;
            left:50%;
            &:after {
                content:"Editor";
            }
        }
    }

}
