.main-wrapper {
    header {
        .title-bar {
            .links {
                padding:5px;
                float:right;
                a {
                    width:24px;
                    height:24px;
                    display:block;
                    background-size:90%;
                    background-repeat:no-repeat;
                    -webkit-transition-property:background-size;
                    -webkit-transition-duration:0.1s;
                    &:hover {
                        background-size:100%;
                    }
                    &.dashboard {
                        background-image:url(@image_dashboard_white);
                    }
                }
            }
        }
    }
}